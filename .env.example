# LLM provider (openai, anthropic, gemini など将来拡張)
PROVIDER=openai
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o-mini # 利用可能モデルへ調整

# Retrieval
HTTP_CACHE_PATH=.cache_http
TIMEOUT_SECONDS=20

# Scoring
ALPHA_S=0.35
BETA_P=0.35
GAMMA_T=0.15
DELTA_L=0.15
SUPPORT_THRESHOLD=0.65
REFUTE_THRESHOLD=0.65
